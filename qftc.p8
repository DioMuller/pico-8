-- title loop
title_loop = {
	init = function()
		music(0)
	end,
	
	update = function()
		if btnp(4) or btnp(5) then
			loop = game_loop
			loop.init()
		end
	end,
	
	draw = function()
			rectfill(0,0,128,128,1)
			
			spr(136,40,40,6,3)
			
			spr(142,0,0)
			spr(143,120,0)
			spr(190,0,120)
			spr(191,120,120)
			
			for x=1,14 do
				spr(158,0,x*8)
				spr(159,120,x*8)
				spr(174,x*8,0)
				spr(175,x*8,120)
			end
			
			print('press 🅾️ or ❎ to start',18,96,7)
	end
}
